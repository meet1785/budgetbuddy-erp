{
  "name": "budgetbuddy-backend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon --watch server/src --ext ts --exec \"ts-node --project server/tsconfig.json\" server/server.ts",
    "build": "tsc -p server/tsconfig.json",
    "start": "node server/dist/server.js",
    "seed": "ts-node --project server/tsconfig.json server/src/utils/seedData.ts",
    "test:mongo": "node test-mongo.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.1",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.16.5",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
